package zplugin.znexusfactions.commands;

import org.bukkit.command.CommandSender;
import org.bukkit.entity.Player;

import zplugin.znexusfactions.zNexusFactions;

public class Open {
	
	public static boolean command(Player player, String[] arguments, zNexusFactions plugin) {
		
		String[] args = new String[arguments.length - 1];

        for (int i = 1; i < arguments.length; i++) {
            args[i - 1] = arguments[i];
        }
		
		return true;
		
	}
	
	private static boolean invalidArgs(CommandSender sender) {
		sender.sendMessage("ยง4Invalid Arguments!");
		sender.sendMessage("ยง2/zfactions list <PAGE>");
		return true;
	}

}
